# Site-specific config (shared vars like Supabase, Google OAuth, PhilSMS are in root .env.local)
BARANGAY_SLUG=your-barangay-slug

# Google OAuth redirect (site-specific port/domain)
GOOGLE_REDIRECT_URI=https://yourdomain.com/api/oauth/callback
GOOGLE_REFRESH_TOKEN=your-refresh-token

# Google Drive folders (site-specific)
GOOGLE_DRIVE_OUTPUT_FOLDER_ID=your-output-folder-id
GOOGLE_DRIVE_PHOTO_FOLDER_ID=your-photo-folder-id

# Google Docs template IDs (site-specific)
BARANGAY_TEMPLATE_ID=your-template-id
RESIDENCY_TEMPLATE_ID=your-template-id
GOOD_MORAL_TEMPLATE_ID=your-template-id
INDIGENCY_TEMPLATE_ID=your-template-id
BUSINESS_TEMPLATE_ID=your-template-id
FACILITY_TEMPLATE_ID=your-template-id
BLOTTER_TEMPLATE_ID=your-template-id
LUNTIAN_TEMPLATE_ID=your-template-id
CSO_ACCREDITATION_TEMPLATE_ID=your-template-id
BARANGAY_ID_TEMPLATE_ID=your-template-id
CSO_ACCREDITATION_PAGE2_TEMPLATE_ID=your-template-id

# PhilSMS (site-specific number)
PHILSMS_NOTIFICATION_NUMBER=your-notification-number
